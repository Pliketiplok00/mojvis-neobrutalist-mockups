{
  "name": "mojvis-backend",
  "version": "0.1.0",
  "type": "module",
  "description": "MOJ VIS Backend API - Fastify + TypeScript",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "lint": "ESLINT_USE_FLAT_CONFIG=false eslint src/**/*.ts",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "keywords": [
    "mojvis",
    "fastify",
    "api"
  ],
  "license": "UNLICENSED",
  "private": true,
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@fastify/cors": "^9.0.1",
    "@fastify/env": "^4.3.0",
    "@fastify/multipart": "^8.3.1",
    "@fastify/static": "^6.12.0",
    "bcrypt": "^6.0.0",
    "dotenv": "^16.4.1",
    "fastify": "^4.26.0",
    "form-data-encoder": "^4.1.0",
    "formdata-node": "^6.0.3",
    "pg": "^8.11.3"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/node": "^20.11.0",
    "@types/pg": "^8.10.9",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "eslint": "^8.56.0",
    "pino-pretty": "^13.1.3",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vitest": "^1.2.0"
  }
}
